        <item name="android:height">16dp</item>
        <item name="android:clickable">true</item>
        <item name="android:paddingTop">@dimen/default_margin_padding_8dp</item>
        <item name="android:paddingBottom">@dimen/default_margin_padding_8dp</item>
        <item name="android:textAllCaps">false</item>
    </style>


